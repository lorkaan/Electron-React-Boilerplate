{
  "name": "Electron-React-Boilerplate",
  "version": "1.0.0",
  "description": "Simply proves that Electron and React can be run together.",
  "main": "dist/electron.bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-react-dev": "webpack --config=react.config.js --mode=development",
    "build-react": "webpack --config=react.config.js --mode=production",
    "build-electron-dev": "webpack --config=electron.config.js --mode=development",
    "build-electron": "webpack --config=electron.config.js --mode=production",
    "build-dev": "npm run build-react-dev && npm run build-electron-dev",
    "build": "npm run build-react && npm run build-electron",
    "start": "electron ."
  },
  "author": "Lorkaan",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "electron": "^3.0.12",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.12.2",
    "npm": "^6.5.0",
    "raw-loader": "^1.0.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
